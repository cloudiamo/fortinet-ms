  <example>
    <content>config router policy
    edit 5001
        set input-device &quot;port1&quot;
        set input-device-negate disable
        set src &quot;172.16.100.0/255.255.255.0&quot;
        set src-negate disable
        set dst &quot;0.0.0.0/0.0.0.0&quot;
        set dst-negate disable
        set action permit
        set protocol 6
        set start-port 443
        set end-port 443
        set start-source-port 0
        set end-source-port 65535
        set gateway 0.0.0.0
        set output-device &quot;port3&quot;
        set tos 0x00
        set tos-mask 0x00
        set status enable
        set comments &apos;&apos;
    next
end</content>
  </example>
