  <example>
    <content>config system link-monitor
    edit &quot;test_Pierre&quot;
        set addr-mode ipv4
        set srcintf &quot;Tunnel13&quot;
        set server &quot;9.9.9.9&quot;
        set protocol ping
        set gateway-ip 0.0.0.0
        set source-ip 81.1.3.116
        set interval 500
        set failtime 5
        set recoverytime 5
        set ha-priority 1
        set update-cascade-interface enable
        set update-static-route enable
        set status enable
    next
    edit &quot;blabla&quot;
        set addr-mode ipv4
        set srcintf &quot;Tunnel12&quot;
